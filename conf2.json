{
	"Sep": ";",
	"Fields": [{
		"Url": "http://rest.db.ripe.net/search.json?query-string=%s",
		"Filter": {
			"inet": {
				"path": ["objects", "object", "attributes", "attribute", "name"],
				"key": "name",
				"value": "inetnum",
				"ret": "value",
				"split": "-"
			},
			"descr": {
				"path": ["objects", "object", "attributes", "attribute", "name"],
				"key": "name",
				"value": "descr",
				"ret": "value"
			},
			"domain": {
				"path": ["objects", "object", "attributes", "attribute", "name"],
				"key": "name",
				"value": "remarks",
				"ret": "value",
				"match": "@",
				"replace": ".*@"
			}
		}
	}],
	"Length": 9,
	"SaveOrder": {
		"ip": 0,
		"inet_0": 1,
		"inet_1": 2,
		"domain": 3
	}
}